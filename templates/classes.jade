{% autoescape off %}

.well.well-small
  h4.boxTitle.well Classes
    a.btn.btn-small.addBtn.btn-primary.pull-right(href="#addClass", data-toggle='modal') &plus;
          

  if(classes.length == 0)
    .alert.alert-warning You don't have any classes!
      a.close(data-dismiss='alert', href='#') &times;

  for class in classes
    .well.well-small=class


{% endautoescape %}