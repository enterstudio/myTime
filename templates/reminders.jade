{% autoescape off %}

.well.well-small.box
  h4.boxTitle.well Tasks and Reminders

    button.btn.btn-small.addBtn.btn-success.pull-right(href="#addReminder", data-toggle='modal')
      i.icon-plus.icon-white
    button.btn.btn-small.addBtn.btn-warning.pull-right#showAllRemind(data-toggle="button")
